<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0; net461</TargetFrameworks>
    <LangVersion>latest</LangVersion>
    <NoWarn>1701;1702;1705;NU1701</NoWarn>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.2.1</Version>
    <PackageReleaseNotes>Commit 293d6ffe</PackageReleaseNotes>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Authors>Habjan</Authors>
    <Company>Habjan</Company>
    <Description>Managed wrapper library around the Ghostscript native library (32-bit &amp;amp; 64-bit), an interpreter for the PostScript language, PDF, related software and documentation.</Description>
    <Copyright>Copyright © 2013-2016 by Josip Habjan (habjan@gmail.com). All Rights Reserved.</Copyright>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>Ghostscript.NET.snk</AssemblyOriginatorKeyFile>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Win32.Registry" Version="4.5.0" />
    <PackageReference Include="System.Drawing.Common" Version="4.5.0" />
  </ItemGroup>
</Project>
